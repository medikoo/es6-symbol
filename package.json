{
  "name": "@github/es6-symbol",
  "version": "3.1.1-github1",
  "description": "ECMAScript 6 Symbol polyfill",
  "author": "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
  "browser": "./dist/polyfill.js",
  "keywords": [
    "symbol",
    "private",
    "property",
    "es6",
    "ecmascript",
    "harmony",
    "ponyfill",
    "polyfill"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/github/es6-symbol.git"
  },
  "publishConfig": {
    "registry": "https://npm.registry.github.com"
  },
  "dependencies": {
    "d": "1",
    "es5-ext": "~0.10.14"
  },
  "devDependencies": {
    "browserify": "~14.3.0",
    "tad": "~0.2.7",
    "xlint": "~0.2.2",
    "xlint-jslint-medikoo": "~0.1.4"
  },
  "scripts": {
    "prepare": "mkdir -p dist && browserify implement.js -o dist/polyfill.js",
    "lint": "node node_modules/xlint/bin/xlint --linter=node_modules/xlint-jslint-medikoo/index.js --no-cache --no-stream",
    "lint-console": "node node_modules/xlint/bin/xlint --linter=node_modules/xlint-jslint-medikoo/index.js --watch",
    "test": "node ./node_modules/tad/bin/tad"
  },
  "license": "MIT"
}
